.singleLine {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; }

.bottomLineModel {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 750rpx;
  height: 200rpx;
  background: #fff;
  font-size: 24rpx;
  color: #515151; }
  .bottomLineModel::before {
    content: "";
    display: inline-block;
    width: 200rpx;
    height: 2rpx;
    margin-right: 8rpx;
    background: #e6e6e6;
    vertical-align: middle; }
  .bottomLineModel::after {
    content: "";
    display: inline-block;
    width: 200rpx;
    height: 2rpx;
    margin-left: 8rpx;
    background: #e6e6e6;
    vertical-align: middle; }

.agreement-box {
  width: 690rpx;
  height: auto;
  padding: 60rpx 30rpx 30rpx;
  background-color: #fff; }
  .agreement-box .title {
    text-align: center;
    font-size: 34rpx;
    color: #2c2c2c;
    font-weight: 600; }
  .agreement-box .info {
    font-size: 28rpx;
    color: #515151;
    text-indent: 2em;
    margin-top: 40rpx; }
