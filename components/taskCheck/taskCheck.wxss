    width: 540rpx;
    padding: 30rpx;
    background-color: #fff; }
    .popUp_groupCheck .content .text {
      display: flex;
      flex-direction: column;
      font-size: 28rpx; }
    .popUp_groupCheck .content .buttons {
      display: flex;
      justify-content: flex-end;
      margin-top: 20rpx; }
      .popUp_groupCheck .content .buttons .button {
        font-size: 28rpx;
        padding: 6rpx 16rpx;
        border: 1rpx solid #e6e6e6;
        margin-left: 20rpx; }
        .popUp_groupCheck .content .buttons .button:active {
          background-color: #e6e6e6; }
        .popUp_groupCheck .content .buttons .button:last-child {
          background-color: #e27b10;
          border: 0;
          color: #fff; }
          .popUp_groupCheck .content .buttons .button:last-child:active {
            opacity: 0.8; }
    .popUp_groupCheck .content .reason {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 100%;
      border: 1rpx solid #e6e6e6;
      margin-top: 20rpx;
      padding: 20rpx 0; }
      .popUp_groupCheck .content .reason textarea {
        width: 480rpx;
        font-size: 24rpx;
        height: 120rpx; }
    .popUp_groupCheck .content .bottomButton {
      display: flex;
      justify-content: space-around;
      margin-top: 30rpx; }
      .popUp_groupCheck .content .bottomButton .button {
        display: flex;
        justify-content: center;
        align-items: center;
        width: 200rpx;
        height: 60rpx;
        border: 1rpx solid #e6e6e6;
        font-size: 32rpx; }
        .popUp_groupCheck .content .bottomButton .button:active {
          background-color: #e6e6e6; }
        .popUp_groupCheck .content .bottomButton .button:last-child {
          background-color: #e27b10;
          color: #fff;
          border: 0; }
          .popUp_groupCheck .content .bottomButton .button:last-child:active {
            opacity: 0.8; }
